{
    "name": "pierreminiggio/tiktok-action-based-api",
    "authors": [
        {
            "name": "Pierre MINIGGIO",
            "email": "pierre@miniggiodev.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "require": {
        "pierreminiggio/config-provider": "dev-main",
        "pierreminiggio/github-action-run-starter-and-artifact-downloader": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev"
    },
    "scripts": {
        "test": "bash vendor/bin/phpunit tests/"
    },
    "scripts-descriptions": {
        "test": "Run all tests!"
    },
}
